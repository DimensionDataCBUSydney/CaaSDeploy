  "template": {
    "metadata": {
      "schemaVersion": "0.1",
      "templateName": "Load balanced VIP for 2 existing servers",
      "templateDescription": "."
    },
    "parameters": {
      "networkDomainId": {
        "description": "The network domain that the VIP should be deployed into.",
        "type": "string",
        "minLength": 36,
        "maxLength": 36,
        "caasResourceId": "NetworkDomain"
      }

    },
    "resources": [
      {
        "resourceType": "Pool",
        "resourceId": "MyPool",
        "dependsOn": [ ],
        "resourceDefinition": {
          "networkDomainId": "$parameters['networkDomainId']",
          "name": "TomsPool",
          "description": "Testing",
          "loadBalanceMethod": "ROUND_ROBIN",
          "serviceDownAction": "RESELECT",
          "slowRampTime": 10
        }
      },
      {
        "resourceType": "VirtualListener",
        "resourceId": "MyVirtualListener",
        "dependsOn": [ "MyPool" ],
        "resourceDefinition": {
          "networkDomainId": "$parameters['networkDomainId']",
          "name": "TomsVL",
          "description": "Testing",
          "type": "STANDARD",
          "protocol": "TCP",
          "listenerIpAddress": "168.128.4.58",
          "port": 80,
          "enabled": true,
          "connectionLimit": 25000,
          "connectionRateLimit": 2000,
          "sourcePortPreservation": "PRESERVE",
          "poolId": "$resources['MyPool'].id",
          "optimizationProfile": [
            "TCP"
          ]

        }
      },

      {
        "resourceType": "Node",
        "resourceId": "MyNode1",
        "dependsOn": [ ],
        "resourceDefinition": {
          "networkDomainId": "$parameters['networkDomainId']",
          "name": "TomsNode1",
          "description": "Testing",
          "ipv4Address": "10.0.0.11",
          "status": "ENABLED",
          "connectionLimit": "20000",
          "connectionRateLimit": "2000"
        }
      },
      {
        "resourceType": "Node",
        "resourceId": "MyNode2",
        "dependsOn": [ ],
        "resourceDefinition": {
          "networkDomainId": "$parameters['networkDomainId']",
          "name": "TomsNode2",
          "description": "Testing",
          "ipv4Address": "10.0.0.12",
          "status": "ENABLED",
          "connectionLimit": "20000",
          "connectionRateLimit": "2000"
        }
      },
      {
        "resourceType": "PoolMember",
        "resourceId": "MyPoolMember1",
        "dependsOn": [ "MyNode1", "MyPool" ],
        "resourceDefinition": {
          "poolId": "$resources['MyPool'].id",
          "nodeId": "$resources['MyNode1'].id",
          "status": "ENABLED",
          "port": 9888
        }
      },
      {
        "resourceType": "PoolMember",
        "resourceId": "MyPoolMember2",
        "dependsOn": [ "MyNode2", "MyPool" ],
        "resourceDefinition": {
          "poolId": "$resources['MyPool'].id",
          "nodeId": "$resources['MyNode2'].id",
          "status": "ENABLED",
          "port": 9888
        }
